<!DOCTYPE html>
<html lang="en">
  <head>
    <% include partials/template/head.ejs %>
    <style>
    .card-img-top {
      width: 160px;
      height: 160px;
    }
    </style>
  </head>
  <body>
    <% include partials/template/header.ejs %>

    <div class="container">
      <div class="row">
        <% users.forEach(function(item) { %>
          <div class="card col-3 ml-2 my-2" style="width: 20rem;">
            <img class="card-img-top" src="<%= item.avatar_url %>" alt="Card image cap">
            <div class="card-block">
              <h4 class="card-title"><%= item.login %></h4>
              <p class="card-text"></p>
              <a href="<%= item.html_url %>" class="btn btn-primary">go page</a>
            </div>
          </div>
        <%  }); %>
      </div>
    </div>

    <% include partials/template/jsdefaults.ejs %>
  </body>
</html>
